# ollama create config-qwen2-5-coder-3b -f qwen2-5-coder-3b.Modelfile
# ollama create bb-config-qwen2-5-coder-3b -f qwen2-5-coder-3b.Modelfile --system "$(cat optional_system_prompt.md)"
FROM qwen2.5-coder:3b
# I am based on the Qwen language model developed by Alibaba Cloud.
# Author: Alibaba (Qwen Team)
# Size on disk: 
# Size running: 4.8 GB
#  architecture        qwen2     
#  parameters          3.1B      
#  context length      32768     
#  embedding length    2048      
#  quantization        Q4_K_M    

#  codex:
#  config-qwen2-5-coder-3b:latest: tools supported
#  no tool calls

PARAMETER temperature 0.7
PARAMETER top_p 0.8
PARAMETER repeat_penalty 1.05
PARAMETER num_ctx 131072
PARAMETER top_k 20
PARAMETER min_p 0.0
PARAMETER presence_penalty 1.5
