# ollama create config-gemma3-4b -f gemma3-4b.Modelfile
# ollama create bb-config-gemma3-4b -f gemma3-4b.Modelfile --system "$(cat optional_system_prompt.md)"
FROM gemma3:4b
# I'm Gemma, a large language model created by the Gemma team at Google DeepMind.
# Author: Google
# Size on disk: 
# Size running: 10 GB
# ollama show config-gemma3-4b && ollama run config-gemma3-4b "what llm model are you"
#  architecture        gemma3    
#  parameters          4.3B      
#  context length      131072    
#  embedding length    2560      
#  quantization        Q4_K_M    

PARAMETER temperature 1.0
PARAMETER top_p 0.95
PARAMETER top_k 64
PARAMETER num_ctx 131072
PARAMETER min_p 0.0
PARAMETER repeat_penalty 1.0
