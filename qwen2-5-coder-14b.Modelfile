# ollama create config-qwen2-5-coder-14b -f qwen2-5-coder-14b.Modelfile
# ollama create bb-config-qwen2-5-coder-14b -f qwen2-5-coder-14b.Modelfile --system "$(cat optional_system_prompt.md)"
FROM qwen2.5-coder:14b
# I am based on the Qwen language model developed by Alibaba Cloud
# Author: Alibaba (Qwen Team)
# Size on disk: 
# Size running: 19 GB
  #  architecture        qwen2     
  #  parameters          14.8B     
  #  context length      32768     
  #  embedding length    5120      
  #  quantization        Q4_K_M    

  # roo:
  #  first run failed eventually
  #  second run looking good
  #  first coding agent tried to one shot it and looped once context full
  #  another agent refused to run commands
  # roo-html:
  #  decent plan but coding agent when whole hog again
  #  agents fell off the rails and looped

PARAMETER temperature 0.7
PARAMETER top_p 0.8
PARAMETER repeat_penalty 1.05
PARAMETER num_ctx 131072
PARAMETER top_k 20
PARAMETER min_p 0.0
PARAMETER presence_penalty 1.5
