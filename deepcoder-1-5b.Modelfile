# ollama create config-deepcoder-1-5b -f deepcoder-1-5b.Modelfile
# ollama create bb-config-deepcoder-1-5b -f deepcoder-1-5b.Modelfile --system "$(cat optional_system_prompt.md)"
FROM deepcoder:1.5b
# Hi! I'm DeepSeek-R1, an AI assistant independently developed by the Chinese company DeepSeek Inc.
# Author: Agentica
# Size on disk: 1.1 GB
# Size running: 5.2 GB
#  architecture        qwen2     
#  parameters          1.8B      
#  context length      131072    
#  embedding length    1536      
#  quantization        Q4_K_M    

PARAMETER temperature 0.6
PARAMETER top_p 0.95
PARAMETER num_predict 64000
PARAMETER num_ctx 32768
