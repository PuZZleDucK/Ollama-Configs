# ollama create config-gemma3n-e2b -f gemma3n-e2b.Modelfile
# ollama create bb-config-gemma3n-e2b -f gemma3n-e2b.Modelfile --system "$(cat optional_system_prompt.md)"
FROM gemma3n:e2b
# I'm Gemma, a large language model created by the Gemma team at Google DeepMind
# Author: Google
# Size on disk: 
# Size running: 6.6 GB
# ollama show config-gemma3n-e2b && ollama run config-gemma3n-e2b "what llm model are you"
#  architecture        gemma3n    
#  parameters          4.5B       
#  context length      32768      
#  embedding length    2048       
#  quantization        Q4_K_M     

PARAMETER temperature 1.0
PARAMETER top_p 0.95
PARAMETER top_k 64
PARAMETER min_p 0.0
PARAMETER repeat_penalty 1.0
PARAMETER num_ctx 32768
